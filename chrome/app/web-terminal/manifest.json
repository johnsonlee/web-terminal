{
  "manifest_version" : 2,

  "name" : "Web Terminal",
  "version" : "1.0",
  "description" : "Web terminal",
  "minimum_chrome_version" : "23",

  "icons" : {
    "16" : "icons/16/terminal.png",
    "48" : "icons/48/terminal.png",
    "128" : "icons/128/terminal.png"
  },

  "app" : {
    "launch" : {
      "local_path" : "terminal.html"
    }
  },

  "permissions" : [
    {
      "socket" : [
        "tcp-connect:*:*"
      ]
    }
  ]
}
